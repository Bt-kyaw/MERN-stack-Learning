<html lang="en">
    <%- include('./components/head') %>
<body>
    
    <%- include('./components/navbar') %>
    
    <div class="container">

        <% if(blogs.length){ %>
            <% blogs.forEach(blog => { %>
                
                 <div class="blog-card">
                    <h1><%= blog.title %></h1>
                    <p> <%= blog.intro  %></p>
                 </div>
                 
    
             <% }) %>
        <% } else { %>
               <h1>There is no blogs.</h1>
            <% } %>

    </div>

   
</body>
</html>